# Copy this file to `.env.local` and fill with real values locally.
# Do NOT commit your real `.env.local` to git.

# LINE
LINE_CHANNEL_SECRET=your_line_channel_secret
LINE_CHANNEL_ACCESS_TOKEN=your_line_channel_access_token
LINE_USER_ID=your_line_user_id
LINE_CHANNEL_ID=your_line_channel_id

# Supabase (public)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_DEFAULT_KEY=your_publishable_key

# Supabase (server)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Admin
ADMIN_COOKIE_SECRET=your_admin_cookie_secret
ADMIN_USER=your_admin_username
ADMIN_PASS=your_admin_password

# App
APP_URL=https://your-deployed-app.example.com/
WEBHOOK_SITE_URL=https://webhook.site/your-id
